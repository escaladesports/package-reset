{
  "name": "package-reset",
  "version": "0.0.0",
  "keywords": [
    "javascript",
    "node",
    "package",
    "reset",
    "git",
    "version",
    "name",
    "repo"
  ],
  "main": "dist-es5/index.js",
  "browser": "dist-es5/index.js",
  "module": "dist-es6/index.js",
  "jsnext:main": "dist-es6/index.js",
  "author": "Kennedy Rose <krose@escaladesports.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/escaladesports/package-reset.git"
  },
  "license": "MIT",
  "scripts": {
    "analyze": "cross-env ANALYZE=1 webpack -p",
    "build": "run-p build:es5 build:es6",
    "build:es6": "cross-env BABEL_ENV=es6 babel ./src --out-dir ./dist-es6",
    "build:es5": "cross-env BABEL_ENV=es5 babel ./src --out-dir ./dist-es5",
    "prepublishOnly": "npm run test && npm run build",
    "test": ""
  },
  "pre-commit": [
    "test"
  ],
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-minify-webpack-plugin": "^0.2.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "chai": "^4.1.2",
    "cross-env": "^5.1.1",
    "mocha": "^4.1.0",
    "npm-run-all": "^4.1.2",
    "pre-commit": "^1.2.2",
    "webpack": "^3.8.1",
    "webpack-bundle-analyzer": "^2.9.1"
  },
  "dependencies": {
    "exeq": "^3.0.0",
    "fs-extra": "^5.0.0",
    "meow": "^4.0.0"
  }
}
